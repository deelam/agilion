
# How concepts are to be imported into Neo4j as nodes or edges
nodeLabels=Person Country
edgeLabels=CITIZEN_OF

# Specifies the identifier that uniquely identifies a concept.
# Syntax: <propertyName>:r.<csvColumnName>
# See Neo4j's Cypher language for reference
Person.idProps=nodeId:r.personId

# Specifies how to populate node properties from CSV file 
# Syntax: n.<propertyName>=r.<csvColumnName>
# See Neo4j's Cypher language for reference
Person.propMapping=\
  n.nodeType='Person', \
  n.nameFirst=r.nameFirst, \
  n.nameLast=r.nameLast, \
  n.countryCitizen=r.countryCitizen, \
  n.nameNick=r.nameNick, \
  n.nameKST=r.nameKST, \
  n.numPass=r.numPass, \
  n.numBiometric=r.numBiometric, \
  n.numFBI=r.numFBI, \
  n.numTide=r.numTide, \
  n.statusLPR=r.statusLPR, \
  n.statusMarital=r.statusMarital, \
  n.statusTIDE=r.statusTIDE, \
  n.statusUSP=r.statusUSP, \
  n.dateBirth=r.dateBirth, \
  n.countryBirth=r.countryBirth, \
  n.citizenNatural=r.citizenNatural, \
  n.flightTraining=r.flightTraining, \
  n.tsaNoFly=r.tsaNoFly, \
  n.visaNum=r.visaNum, \
  n.visaStatusRevoke=r.visaStatusRevoke, \
  n.terrGroupMem=r.terrGroupMem, \
  n.tideCateg=r.tideCateg, \
  n.title=r.title, \
  n.dataSourceTIDE=r.dataSource

# Specifies which property name should be unique. 
# Multivalue names are separated by a comma.
Person.uniqueProps=nodeId

# Specifies which properties to index for lookup
Person.propsToIndex=countryCitizen

Country.idProps=nodeId:r.countryId
Country.propMapping=\
  n.nodeType='Country', \
  n.nameCountry=r.countryCitizen
Country.uniqueProps=nodeId

# For concepts that will be an edge, specify the from and to nodes.
# Syntax: <Concept> {<idProp>: r.<csvColumnName>}
# See Neo4j's Cypher language for reference
CITIZEN_OF.fromNode=Person {nodeId: r.personId}
CITIZEN_OF.toNode=Country {nodeId: r.countryId}
CITIZEN_OF.propMapping=\
  n.countryId=r.countryId, \
  n.dataSourceTIDE=r.dataSource

